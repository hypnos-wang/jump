{
    "inbounds": 
    [
        {
            "listen": "/etc/caddy/less",
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {"id": "$UUID"}
                ],
                "disableInsecureEncryption": false
            },
            "streamSettings": {"network": "ws","wsSettings": {"path": "$PATH"}}
        }
    ],
    
    "outbounds": 
    [
        {"protocol": "freedom","tag": "direct","settings": {}}
    ]
    
}
